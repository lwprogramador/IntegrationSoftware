	<subDataset name="Table Dataset 1" uuid="2d75dbd1-1968-4092-be82-351e879136fe">
		<parameter name="APRIETE" class="java.lang.String">
			<defaultValueExpression><![CDATA["0"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DISTINCT
ald.tx_apriete as det_apriete,
(SELECT iald.num_val_apriete AS det_val_apriete FROM herramientas.tbl_aprietes_logs_detalle AS iald WHERE iald.idaprietelog = ald.idaprietelog AND iald.tx_apriete = ald.tx_apriete ORDER BY iald.fecha DESC LIMIT 1),
(SELECT CASE WHEN iald.tx_condicion = 'ALTA' THEN iald.linea2 ELSE iald.linea1 END AS det_val_apli_apriete FROM herramientas.tbl_aprietes_logs_detalle AS iald WHERE iald.idaprietelog = ald.idaprietelog AND iald.tx_apriete = ald.tx_apriete ORDER BY iald.fecha DESC LIMIT 1),
(SELECT iald.auxiliar1 AS det_auxiliar_1 FROM herramientas.tbl_aprietes_logs_detalle AS iald WHERE iald.idaprietelog = ald.idaprietelog AND iald.tx_apriete = ald.tx_apriete ORDER BY iald.fecha DESC LIMIT 1),
(SELECT iald.auxiliar2 AS det_auxiliar_2 FROM herramientas.tbl_aprietes_logs_detalle AS iald WHERE iald.idaprietelog = ald.idaprietelog AND iald.tx_apriete = ald.tx_apriete ORDER BY iald.fecha DESC LIMIT 1)
FROM
herramientas.tbl_aprietes_logs_detalle ald

WHERE
ald.idaprietelog  = $P{APRIETE}::integer ORDER BY ald.tx_apriete ASC]]>
		</queryString>
		<field name="det_apriete" class="java.lang.String"/>
		<field name="det_val_apriete" class="java.math.BigDecimal"/>
		<field name="det_val_apli_apriete" class="java.math.BigDecimal"/>
		<field name="det_auxiliar_1" class="java.math.BigDecimal"/>
		<field name="det_auxiliar_2" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="PAHT_ICON" class="java.lang.String"/>
	<parameter name="AJUSTE" class="java.lang.String"/>
	<parameter name="PAHT_GRAFICA" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
lpad(al.id::text, 8,'0') as nro_reporte,
al.cliente AS cli_reporte,
h.tx_district as codigo_herr_reporte,
h.tx_nombre AS herr_reporte,
h.tx_serial AS ser_herr_reporte,
to_char(al.fecha, 'dd/MM/YYYY') as fecha_reporte,
al.odt AS odt_reporte,
CONCAT(o.tx_nombre, ' ', o.tx_apellido) as op_reporte,
p.tx_nombre as patron_equipo,
p.tx_marca as patron_marca,
p.tx_modelo as patron_modelo,
p.tx_serie as patron_serie,
p.fe_ultima_calibracion as patron_fechacal

from
herramientas.tbl_aprietes_logs AS al
INNER JOIN herramientas.tbl_herramientas AS h ON al.idherramienta = h.id
INNER JOIN entidades.tbl_operador AS o ON o.id = al.idoperador
inner join entidades.tbl_patron  as p on p.id = al.patron

where al.id = $P{AJUSTE}::integer]]>
	</queryString>
